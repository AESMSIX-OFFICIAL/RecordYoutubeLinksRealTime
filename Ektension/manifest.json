{
  "manifest_version": 2,
  "name": "Youtube Logger",
  "version": "3.0",
  "description": "automatically record youtube links with python",
"permissions": [
  "tabs",
  "storage",
  "webNavigation",
  "webRequest",
  "<all_urls>"
],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "YouTube Logger",
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
